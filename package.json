{
	"name": "nlevel",
	"description": "node.js odm for leveldb",
	"version": "0.2.0",
	"author": "Oleg Korobenko <oleg.korobenko@gmail.com>",
	"repository": {
		"type": "git",
		"url": "git://github.com/okv/nlevel"
	},
	"keywords": [
		"leveldb", "level", "store", "storage", "db", "database", "in-process", "sqlight"
	],
	"scripts": {
		"test": "mocha test --reporter spec --bail --async-only",
		"benchmark": "mocha benchmark/*.bench.js --reporter spec --bail --async-only --slow 1 --timeout 30000",
		"doc": "dox --api < lib/index.js | sed -e 's/##/###/g'"
	},
	"dependencies": {
		"level": "0.18.0"
	},
	"devDependencies": {
		"mocha": "1.11.0",
		"expect.js": "0.2.0",
		"dox": "0.4.4"
	},
	 "main": "./lib/index"
}
